{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "\u256d\u2500[<#55ff55>Green_OnionsðŸ§…</>]\u2500",
          "type": "session"
        },
        {
          "foreground": "#ffff00",
          "style": "plain",
          "template": "<#ffffff>[</>{{ .Path }}<#ffffff>]</>",
          "type": "path"
        },
        {
          "foreground": "#ff0000",
          "style": "plain",
          "template": "<#ffffff>\u2500(</>#<#ffffff>)</>",
          "type": "root"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#55ffff",
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "style": "diamond",
          "template": "<#ffffff>[</>{{ .FormattedMs }}s \ue601<#ffffff>]\u2500</>",
          "type": "executiontime"
        },
        {
          "foreground": "#55ffff",
          "style": "diamond",
          "template": "<#ffffff>[</><#ffffff>RAM:</> {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1000000000.0) }}GB <#ffffff>]\u2500</>",
          "type": "sysinfo"
        },
        
        {
          "foreground": "#ffffff",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#FFFB38{{end}}",
            "{{if eq \"Full\" .State.String}}#33DD2D{{end}}"
          ], 
          "properties": {
            "charged_icon": "\uf58e ",
            "charging_icon": "âš¡ ",
            "discharging_icon": "\ue234 "
          },
          "style": "plain",
          "template": "<#ffffff>[</>{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}%<#ffffff>]\u2500</>",
          "type": "battery"
        },
        {
          "foreground": "#55ffff",
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": "<#ffffff>[</>\u231b{{ .CurrentDate | date .Format }}<#ffffff>]</>",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        },
        {
          "foreground": "#ffffff",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<#ffffff>[</>{{ .HEAD }}{{ if .Staging.Changed }}<#00AA00> \u25cf {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#D75F00> \u25cf {{ .Working.String }}</>{{ end }}<#ffffff>]-</>",
          "type": "git"
        },
        {
          "foreground": "#fff703",
          "foreground_templates": ["{{ if gt .Code 0 }}#ef5350{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "<#ffffff>(</>Â¯\\_(ãƒ„)_/Â¯<#ffffff>)</>ðŸ’² ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}